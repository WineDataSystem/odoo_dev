<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
	<t t-name="WDS_Core_SearchExtension">
		<div class="ui-toolbar" style="margin-bottom:0px;">
			<div class="oe_form_char_section">
				<div class="oe_form_dropdown_section">
					<h4>Vintage :</h4>
					<select multiple="multiple" class="oe_select_vintage"></select>
				</div>
			</div>
		</div>
	</t>
	<t t-name="WDS_Core_SearchvariantExtension">
		<div class="ui-toolbar"  style="margin-bottom:0px;">
			<div class="oe_form_char_section">
				<div class="oe_form_dropdown_section">
					<h4>Vintage :</h4>
					<select style="font-size:5;" multiple="multiple" class="oe_select_variant"></select>
				</div>
			</div>
		</div>
	</t>
	<t t-name="WDS_Core_SearchvariantPopupExtension">
		<div class="ui-toolbar"  style="margin-bottom:0px;">
			<div class="oe_form_char_section">
				<div class="oe_form_dropdown_section">
					<h4>Vintage :</h4>
					<select style="font-size:9;" multiple="multiple" class="oe_select_variant"></select>
				</div>
			</div>
		</div>
	</t>
</templates>





